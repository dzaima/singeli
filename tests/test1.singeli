f{A,B}(a:A, b:B) : A {
  a+b
}
g{T,n}(a:T) {
  x:T = n+a;
  x
}
h1{T,n}(a:[n]T, b:[n]T) : [n]T {
  a + b
}
h2{T}(a:T, b:T) : [n]T {
  a + b
}
h3{A,B}(a:A, b:B) : [n]T {
  a + b
}

'f1' = f{i32,i32};
'f2' = f{i32,i64};
'f3' = f{i32};
'g1' = g{i32, 5};
'g2' = g{i32, 1*2 + 3*4};
'h1' = h1{i32,4};
'h2' = h2{[4]i32};
'h3' = h3{[4]i32, [8]i32};