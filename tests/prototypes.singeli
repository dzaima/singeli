safe_double{T} (x:T) {
  y:2*T = Cast{2*T}(x);
  y + y
}

safe_double{f64} (x:f64) => x+x


mulvv{T,S | S<T}(n:Size, z:*T, w:*S, x:*T) : Size {
  @for_vec{T} w, x, z over i to n {
    z = Cast{T}(w) *@{return i;} x;
  }
  n
}

mulvv_over{T,S,O | S<T & O>T}(n:Size, z:*O, w:*S, x:*T) : Size {
  @for_vec{O} w, x, z over i to n {
    z = Cast{O}(w) * Cast{O}(x);
  }
  n
}
